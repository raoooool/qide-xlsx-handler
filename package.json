{
  "name": "qide-xlsx-handler",
  "productName": "培训报告小能手",
  "version": "1.0.0",
  "main": "dist/main.js",
  "license": "MIT",
  "scripts": {
    "bootstrap": "yarn && cd web && yarn",
    "dev": "tsc -w",
    "start": "export ELECTRON_NODE_ENV=dev && electron-forge start",
    "package": "electron-forge package",
    "make": "electron-forge make"
  },
  "devDependencies": {
    "exceljs": "^4.3.0",
    "fs-extra": "^10.1.0",
    "@types/fs-extra": "^9.0.13",
    "@electron-forge/cli": "^6.0.0-beta.66",
    "@electron-forge/maker-dmg": "^6.0.0-beta.66",
    "electron": "^20.1.4",
    "ts-node": "^10.9.1",
    "typescript": "^4.8.3"
  },
  "config": {
    "forge": {
      "packagerConfig": {
        "icon": "public/icon.icns"
      },
      "makers": [
        {
          "name": "@electron-forge/maker-dmg",
          "config": {
            "format": "ULFO",
            "icon": "public/icon.icns"
          }
        }
      ]
    }
  }
}
